#include <stdio.h>
#include <stdlib.h>

typedef struct node{
    int priority;
    int num;
    struct node *next;
}node;

typedef struct priority_queue{
    node *head;
}p_queue;

void insert_priority_queue(int n, p_queue *q, int priority){
    node *aux = q->head;
    node *new = malloc(sizeof(node));
    new->num = n;
    new->priority = priority;
    new->next = NULL;

    if(q->head == NULL){
        q->head = new;
        return;
    }
    while(aux->next && aux->next->priority >= new->priority){
        aux = aux->next;
    }
    new->next = aux->next;
    aux->next = new;
}

void pop(p_queue *q){
    node *aux = q->head;
    if (q->head) {
        q->head = q->head->next;
        free(aux);
    }
}

int top(p_queue *q){
    return q->head->num;
}

int main(){
    return 0;
}
